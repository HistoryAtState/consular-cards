    <text corresp="#c1473">
        <body>
            <div corresp="#c1473f1">
                <head>Stoke-on-Trent, England. (formerly Burslem)</head>
                <table>
                    <row role="label">
                        <cell>PREV. OFFICE.</cell>
                        <cell>NAME.</cell>
                        <cell>OFFICE.</cell>
                        <cell>ALLEGIANCE.</cell>
                        <cell>WHERE BORN.</cell>
                        <cell>WHENCE APPTD.</cell>
                        <cell>DATE OF APPOINTMENT.</cell>
                        <cell>REMARKS.</cell>
                    </row>
                    <row>
                        <cell>London</cell>
                        <cell>Edwin A. Long</cell>
                        <cell>Vice Con U.S.</cell>
                        <cell>U.S.</cell>
                        <cell><unclear>Haute</unclear> Ind.</cell>
                        <cell>Ind. Nov. 6, 1919.</cell>
                        <cell>Appts V.C., Cardiff Mar. 23, 1920 <lb/> to Apr. 7, 1920</cell>
                    </row>
                    <row>
                        <cell>London</cell>
                        <cell>William N. Carroll</cell>
                        <cell>" "</cell>
                        <cell>" "</cell>
                        <cell>N.C.</cell>
                        <cell>N.C.</cell>
                        <cell>Feb. 29, 1924.</cell>
                        <cell>Appts V.C., London, May 1, 1924.</cell>
                    </row>
                    <row>
                        <cell>London</cell>
                        <cell>Daniel Miller</cell>
                        <cell>" "</cell>
                        <cell>" "</cell>
                        <cell>Md.</cell>
                        <cell>Md.</cell>
                        <cell>Aug 21, 1925.</cell>
                        <cell>Appts V.C. London Dec. 11, 1925 <lb/> to Sept. 20, 1926.</cell>
                    </row>
                    <row>
                        <cell>Bradford</cell>
                        <cell>George L. Fleming</cell>
                        <cell>" "</cell>
                        <cell>" "</cell>
                        <cell>Ky.</cell>
                        <cell>Mo.</cell>
                        <cell>July 22, 1926</cell>
                        <cell>Apptd V.C. Bradford Sept. 20, 1926.</cell>
                    </row>
                    <row>
                        <cell>Swansea</cell>
                        <cell>Clarence <unclear>Trick</unclear></cell>
                        <cell>" "</cell>
                        <cell>" "</cell>
                        <cell>Nebr.</cell>
